<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle"
    android:layout_width="80dp"
    android:layout_height="60dp">
    <corners
        android:radius="14dp"
        />
    <solid
        android:color="#94B0FF"
        />
    <padding
        android:left="0dp"
        android:top="0dp"
        android:right="0dp"
        android:bottom="0dp"
        />
    <size
        android:layout_width="80dp"
        android:layout_height="60dp"
        />
    <stroke
        android:width="3dp"
        android:color="#FFF8F0"
        />
</shape>
